<div id="zotero-collections">
    <ul class="zotero-items">
        {% for item in elements['elements'] %}
            <li class="zotero-item">
                <a href="{{ item['links']["alternate"]['href'] }}"><h4 class="zotero-item__title">{{ item['data']['title']}}</h4></a>
                <ul class="actions">
                    <li><a href="{{ path('entity.research_reference_entity.add_page', item) }}" class="zotero-item__import">Import this item</a></li>
                </ul>
            </li>
        {% endfor %}
    </ul>
</div>
